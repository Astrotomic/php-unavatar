{
    "name": "astrotomic/php-unavatar",
    "description": "PHP integration of unavatar service.",
    "keywords": [
        "astrotomic",
        "unavatar",
        "avatar"
    ],
    "homepage": "https://astrotomic.info",
    "license": "MIT",
    "authors": [
        {
            "name": "Tom Witkowski",
            "email": "gummibeer@astrotomic.info",
            "homepage": "https://gummibeer.de",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.2"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Astrotomic\\Unavatar\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Astrotomic\\Unavatar\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "support": {
        "email": "dev@astrotomic.info",
        "issues": "https://github.com/Astrotomic/php-unavatar/issues",
        "source": "https://github.com/Astrotomic/php-unavatar"
    }
}
